{
  "name": "code-chatgpt-assistant",
  "displayName": "BitGPT",
  "description": "一个简单的 ChatGPT VSCode 插件",
  "version": "1.2.0",
  "author": "公众号：古时的风筝",
  "engines": {
    "vscode": "^1.66.0"
  },
  "license": "BSD-3-Clause",
  "repository": {
    "type": "github",
    "url": "https://github.com/huzhicheng/vscode-ChatGPT"
  },
  "categories": [
    "Other"
  ],
  "icon": "resources/icon.png",
  "publisher": "fengzheng",
  "activationEvents": [],
  "main": "./dist/extension.js",
  "contributes": {
    "viewsContainers": {
      "activitybar": [
        {
          "id": "bitGPT",
          "title": "BitGPT",
          "icon": "resources/cat.svg"
        }
      ]
    },
    "views": {
      "bitGPT": [
        {
          "type": "webview",
          "id": "bitGPT.chat",
          "name": "just a code assistant"
        }
      ]
    },
    "commands": [
      {
        "command": "fengzheng.chatgpt",
        "title": "Chat",
        "icon": {
          "light": "resources/cat.svg",
          "dark": "resources/cat.svg"
        }
      },
      {
        "command": "fengzheng.chatgpt.inputKey",
        "title": "ChatGPT API"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "yarn run package",
    "compile": "webpack",
    "watch": "webpack --watch",
    "package": "webpack --mode production --devtool hidden-source-map",
    "compile-tests": "tsc -p . --outDir out",
    "watch-tests": "tsc -p . -w --outDir out",
    "pretest": "yarn run compile-tests && yarn run compile && yarn run lint",
    "lint": "eslint src --ext ts",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@types/glob": "^8.1.0",
    "@types/mocha": "^10.0.1",
    "@types/node": "16.x",
    "@types/vscode": "^1.66.0",
    "@typescript-eslint/eslint-plugin": "^5.56.0",
    "@typescript-eslint/parser": "^5.56.0",
    "@vscode/test-electron": "^2.3.0",
    "eslint": "^8.36.0",
    "glob": "^8.1.0",
    "mocha": "^10.2.0",
    "ts-loader": "^9.4.2",
    "typescript": "^4.9.5",
    "webpack": "^5.76.3",
    "webpack-cli": "^5.0.1"
  },
  "dependencies": {
    "axios": "^1.3.6"
  }
}
